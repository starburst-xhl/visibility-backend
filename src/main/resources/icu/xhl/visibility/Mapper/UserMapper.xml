<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="icu.xhl.visibility.Mapper.UserMapper">
    <select id="selectAll" resultType="icu.xhl.visibility.Entity.User">
        select * from users
    </select>
    <select id="selectById" resultType="icu.xhl.visibility.Entity.User">
        select * from users where id = #{id}
    </select>
</mapper>